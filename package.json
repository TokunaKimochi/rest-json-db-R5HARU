  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "dotenvx run -f .env.postgresql -f .env.production -- node dist\\assets\\bundle.js",
    "dev": "dotenvx run -f .env.postgresql -f .env.development -- ts-node-dev --require tsconfig-paths/register --ignore-watch=vendor -- src\\index.ts",
    "build": "dotenvx run -f .env.postgresql -f .env.production -- rollup -c",
    "tsc": "dotenvx run -f .env.postgresql -f .env.production -- tsc && tsc-alias",
    "lint": "eslint src/ --ext .js,.ts --fix",
    "format": "prettier --check \"**/*.+(js|json|yml|ts)\" || prettier --write \"**/*.+(js|json|yml|ts)\"",
    "test": "dotenvx run -f .env.postgresql -f .env.development -- jest --watch",
    "test:full": "set INSERT_ENABLED=true && dotenvx run -f .env.postgresql -f .env.development -- jest",
    "test:cov:full": "set INSERT_ENABLED=true && dotenvx run -f .env.postgresql -f .env.development -- jest --coverage",
    "test:db:ship": "set INSERT_ENABLED=true && dotenvx run -f .env.postgresql -f .env.development -- jest src/routes/shipping-instruction-printouts/shippingInstructionPrintouts.test.ts",
    "api:geoUp": "node dist\\scripts\\geoJpApiUpdate.js",
    "api:postalUp": "node dist\\scripts\\ejPostalApiUpdate.js",
    "sql:customersUp": "dotenvx run -f .env.postgresql -f .env.production -- node dist\\scripts\\updateCustomersTable.js",
    "sql:reCheck": "dotenvx run -f .env.postgresql -f .env.production -- node dist\\scripts\\reCheckValidationCustomersTable.js",
    "rollout": "dotenvx run -f .env.postgresql -f .env.production -- node dist\\scripts\\node-windows\\svc-install.js",
    "withdraw": "dotenvx run -f .env.postgresql -f .env.production -- node dist\\scripts\\node-windows\\svc-uninstall.js"
  },
